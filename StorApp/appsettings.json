{

  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "Microsoft.AspNetCore": "Debug",
      "StorApi.Controllers": "Debug"
    }
  },
  "ConnectionStrings": {
    "DefaultConnection": "Data Source = (localdb)\\MSSQLLocalDB; Initial Catalog = StorDB"
  },
  "mailStrings": {
    "mailTo": "admin@gmail.com",
    "mailFrom": "anything@gmail.com"

  },
 "Serilog": {
    "MinimumLevel": "Information",
    "WriteTo": [
      {
        "Name": "MSSqlServer",
        "Args": {
          "connectionString": "Data Source = (localdb)\\MSSQLLocalDB; Initial Catalog = StorDB",
          "tableName": "Log",
          "autoCreateSqlTable": true
        }
      }
    ]
  },
  "AllowedHosts": "*"
}